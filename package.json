{
  "name": "websocket-server",
  "version": "0.1.1",
  "private": true,
  "main": "electron/index.js",
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@types/jest": "24.0.11",
    "@types/material-ui": "^0.21.6",
    "@types/node": "11.13.6",
    "@types/react": "16.8.14",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "^4.3.2",
    "concurrently": "^4.1.0",
    "emotion": "^10.0.9",
    "final-form": "^4.12.0",
    "final-form-material-ui": "^0.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-final-form": "^4.1.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "socket.io": "^2.2.0",
    "typescript": "3.4.4"
  },
  "scripts": {
    "react": "react-scripts start",
    "build:react": "react-scripts build",
    "build:electron:win:x86": "electron-builder -w --ia32",
    "build:electron:win:x64": "electron-builder -w --x64",
    "build:electron:win": "yarn build:electron:win:x86 && yarn build:electron:win:x64",
    "build:electron:all": "electron-builder -wm",
    "build:all": "yarn build:react && yarn build:electron:all",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "all": "concurrently \"yarn react\" \"yarn electron\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-builder": "^20.39.0"
  },
  "build": {
    "productName": "WebsocketServer",
    "extends": null,
    "files": [
      "build/**/*",
      "electron/**/*",
      "package.json"
    ],
    "directories": {
      "output": "dist/${productName}-${platform}-${arch}"
    },
    "portable": {
      "artifactName": "WebsocketServer_${version}.${ext}"
    },
    "win": {
      "target": "portable"
    },
    "mac": {
      "target": "dmg"
    }
  }
}
